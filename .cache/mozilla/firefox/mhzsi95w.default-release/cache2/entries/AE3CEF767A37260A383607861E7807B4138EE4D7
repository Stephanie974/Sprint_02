                // Pas de médias, cherchons un VAST
                if (data.hasOwnProperty('activity_id') && data.activity_id != '') {
                    rwmpVars.globalVast = 'https\u003A\/\/publishers.edisound.com\/api\/p\/medias\/vast/'+data.activity_id;
                    if (params.edisound_preroll_preview) {
                        rwmpVars.globalVast = rwmpVars.globalVast + '/force_preview/' + params.edisound_preroll_preview
                    }
                }
                console.log('Missing sponsorized songs');
                data = {"medias":[]};
            }
            rwmpLoadPlayer(data.medias);
        })
        .catch(
            (err) => {
                console.log('Fetch https\u003A\/\/publishers.edisound.com\/api\/p\/medias\/fetch error : ', err);
                console.log('An error occured. Your podcast will be displayed anyway');
                rwmpLoadPlayer([]);
            }
        );
    
