        .then(res => res.json())
        .then((data) => {
            if (data.hasOwnProperty('user_id') && data.user_id != null && getCookie('_edisound_player') == null) {
                setCookie('_edisound_player', data.user_id, 365);
            }
            else if (getCookie('_edisound_player')) {
                setCookie('_edisound_player', getCookie('_edisound_player'), 365);
            }
            if (data.hasOwnProperty('yknytmzg') && data.yknytmzg === 1) {
                rwmpVars.yknytmzg = true;
            }
